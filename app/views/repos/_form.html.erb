<% form_for @repo do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :event_id %><br />
    <%= f.text_field :event_id %>
  </p>
  <p>
    <%= f.label :watchers %><br />
    <%= f.text_field :watchers %>
  </p>
  <p>
    <%= f.label :network %><br />
    <%= f.text_field :network %>
  </p>
  <p>
    <%= f.label :wiki %><br />
    <%= f.text_field :wiki %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
